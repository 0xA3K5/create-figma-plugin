{
  "name": "@create-figma-plugin/build",
  "version": "0.0.85-alpha.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Build a Figma plugin, with support for multiple menu commands that each have their own UI implementation",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/build"
  },
  "files": [
    "lib"
  ],
  "bin": {
    "build-figma-plugin": "lib/cli.js"
  },
  "scripts": {
    "build": "yarn run clean-build && tsc --build",
    "clean": "yarn run clean-build && yarn run clean-test",
    "clean-build": "rm -rf '*.log' lib tsconfig.tsbuildinfo",
    "clean-test": "rm -rf '*.log' 'test/fixtures/*/build' .nyc_output coverage",
    "fix": "eslint --fix '{src,test}/**/*.ts' && prettier --loglevel error --write '*.json' 'test/fixtures/**/*.json'",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "yarn run build && yarn run clean-test && tap 'test/*.ts' --coverage-report html --coverage-report text --no-browser --no-check-coverage --reporter terse --timeout 120 --ts"
  },
  "dependencies": {
    "@create-figma-plugin/common": "^0.0.85-alpha.5",
    "@create-figma-plugin/tsconfig": "^0.0.85-alpha.4",
    "chokidar": "^3.5.1",
    "cssnano": "^5.0.2",
    "esbuild": "^0.11.16",
    "execa": "^5.0.0",
    "find-up": "^5.0.0",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "kleur": "^4.1.4",
    "postcss": "^8.2.13",
    "postcss-modules": "^4.0.0",
    "rev-hash": "^3.0.0",
    "sade": "^1.7.4",
    "temp-write": "^4.0.0",
    "typed-css-modules": "^0.6.8"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/sade": "^1.7.2",
    "@types/tap": "^15.0.0",
    "eslint": "^7.25.0",
    "eslint-config-yuanqing": "^0.0.5",
    "preact": "^10.5.13",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tap": "^15.0.5",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@figma/plugin-typings": "^1",
    "typescript": "^4"
  }
}
