{
  "name": "@create-figma-plugin/utilities",
  "version": "0.0.85-alpha.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Helpful utility functions for common Figma plugin operations",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/utilities"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean-build && yarn run generate-index-ts && tsc --module ES2020 --moduleResolution Node",
    "clean": "yarn run clean-build && yarn run clean-test",
    "clean-build": "rm -rf '*.log' lib tsconfig.tsbuildinfo",
    "clean-test": "rm -rf '*.log' .nyc_output coverage",
    "fix": "eslint --fix '{scripts,src,test}/**/*.ts' && prettier --loglevel error --write '*.json'",
    "generate-docs": "ts-node scripts/generate-docs.ts",
    "generate-index-ts": "ts-node ../../scripts/generate-index-ts.ts 'src/**/*.ts' && eslint --fix 'src/index.ts'",
    "lint": "eslint '{scripts,src,test}/**/*.ts'",
    "test": "yarn run build && yarn run clean-test && tap 'test/**/*.ts' --coverage-report html --coverage-report text --no-browser --no-check-coverage --reporter terse --timeout 120 --ts"
  },
  "dependencies": {
    "natural-compare-lite": "1.4.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1",
    "@types/fs-extra": "^9.0.11",
    "@types/natural-compare-lite": "^1.4.0",
    "@types/node": "^15.0.1",
    "@types/tap": "^15.0.0",
    "eslint": "^7.25.0",
    "eslint-config-yuanqing": "^0.0.4",
    "fs-extra": "^9.1.0",
    "generate-ts-docs": "^0.0.6",
    "prettier": "^2.2.1",
    "tap": "^15.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "sideEffects": false
}
