{
  "name": "@create-figma-plugin/ui",
  "version": "0.0.69",
  "publishConfig": {
    "access": "public"
  },
  "description": "A library of Preact components that replicate the Figma UI design",
  "keywords": [
    "components",
    "component-library",
    "create-figma-plugin",
    "design-system",
    "figma",
    "figma-plugin",
    "figma-plugin-ds",
    "figma-plugins",
    "figma-ui",
    "forms",
    "preact",
    "ui",
    "ui2"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/ui"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "scripts": {
    "build": "yarn run clean && mkdirp node_modules && build-storybook --output-dir build",
    "clean": "rimraf build",
    "deploy": "yarn run clean && storybook-to-ghpages --out build",
    "fix": "prettier-standard '{.storybook,src}/**/*.ts' && prettier --write '*.json'",
    "lint": "standardx '{.storybook,src}/**/*.ts'",
    "watch": "yarn run clean && mkdirp node_modules && start-storybook"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^0.0.69",
    "@sindresorhus/class-names": "^1.1.0"
  },
  "peerDependencies": {
    "preact": "^10.4.1"
  },
  "sideEffects": [
    "./src/scss/{base,font-face,reset}.scss"
  ],
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "yuanqing",
    "gitEmail": "github@yuanqing.sg",
    "commitMessage": "Deploy Storybook"
  }
}
