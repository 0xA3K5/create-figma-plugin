{
  "name": "@create-figma-plugin/ui",
  "version": "0.0.85-alpha.7",
  "publishConfig": {
    "access": "public"
  },
  "description": "A library of Preact components that replicate the Figma UI design",
  "keywords": [
    "components",
    "component-library",
    "create-figma-plugin",
    "design-system",
    "figma",
    "figma-plugin",
    "figma-plugin-ds",
    "figma-plugins",
    "figma-ui",
    "forms",
    "preact",
    "ui",
    "ui2"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/ui"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean && yarn run generate-index-ts && tsc --module ES2020 --moduleResolution Node && ts-node scripts/copy-non-ts-files.ts",
    "clean": "rm -rf '*.log' lib 'src/**/*.css.d.ts' tsconfig.tsbuildinfo",
    "fix": "eslint --fix '{.storybook,scripts,src}/**/*.{js,ts,tsx}' && stylelint --fix 'src/**/*.css' && prettier --loglevel error --write '*.json' '.storybook/**/*.html'",
    "generate-icons": "ts-node scripts/generate-icons.ts && eslint --fix 'src/components/icon/**/*.{ts,tsx}'",
    "generate-index-ts": "ts-node ../../scripts/generate-index-ts.ts 'src/**/*.{ts,tsx}' '!src/**/private/**/*' && eslint --fix 'src/index.ts'",
    "lint": "eslint '{.storybook,scripts,src}/**/*.{js,ts,tsx}' && stylelint 'src/**/*.css'",
    "storybook-build": "yarn run storybook-clean && mkdirp node_modules && build-storybook --output-dir build",
    "storybook-clean": "rm -rf '*.log' build 'src/**/*.css.d.ts'",
    "storybook-watch": "yarn run storybook-clean && mkdirp node_modules && start-storybook"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^0.0.85-alpha.6"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@storybook/addon-storysource": "^6.2.9",
    "@storybook/preact": "^6.2.9",
    "@storybook/source-loader": "^6.2.9",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^15.0.1",
    "@types/svgo": "^2.3.0",
    "change-case": "^4.1.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "eslint-config-yuanqing": "^0.0.5",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "mkdirp": "^1.0.4",
    "preact": "^10.5.13",
    "prettier": "^2.2.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.0",
    "stylelint-config-yuanqing": "^0.0.1",
    "svgo": "^2.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.36.1"
  },
  "peerDependencies": {
    "preact": "^10"
  },
  "sideEffects": false
}
