{
  "name": "@create-figma-plugin/ui",
  "version": "0.0.85-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A library of Preact components that replicate the Figma UI design",
  "keywords": [
    "components",
    "component-library",
    "create-figma-plugin",
    "design-system",
    "figma",
    "figma-plugin",
    "figma-plugin-ds",
    "figma-plugins",
    "figma-ui",
    "forms",
    "preact",
    "ui",
    "ui2"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/ui"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean && tsc --module es2020 --moduleResolution node && ts-node scripts/copy-non-ts-files.ts",
    "clean": "rimraf '*.log' lib 'src/**/*.css.d.ts' tsconfig.tsbuildinfo",
    "fix": "eslint --fix '{.storybook,scripts,src}/**/*.{ts,tsx}' && stylelint --fix 'src/**/*.css' && prettier --loglevel error --write '*.json' '.storybook/**/*.html'",
    "lint": "eslint '{.storybook,scripts,src}/**/*.{ts,tsx}' && stylelint 'src/**/*.css'",
    "storybook-build": "yarn run storybook-clean && mkdirp node_modules && build-storybook --output-dir build",
    "storybook-clean": "rimraf '*.log' build 'src/**/*.css.d.ts'",
    "storybook-watch": "yarn run storybook-clean && mkdirp node_modules && start-storybook"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^0.0.85-alpha.0",
    "@sindresorhus/class-names": "^1.1.0"
  },
  "devDependencies": {
    "@storybook/addon-storysource": "^6.2.7",
    "@storybook/preact": "^6.2.7",
    "@storybook/source-loader": "^6.2.7",
    "@types/fs-extra": "^9.0.10",
    "@types/node": "^14.14.37",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-yuanqing": "^0.0.4",
    "fs-extra": "^9.1.0",
    "mkdirp": "^1.0.4",
    "preact": "^10.5.13",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "preact": "^10"
  },
  "sideEffects": [
    "./lib/css/*.css"
  ],
  "eslint": {
    "ignore": [
      "src/**/*.css.d.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "storybook-deployer": {
    "gitUsername": "yuanqing",
    "gitEmail": "github@yuanqing.sg",
    "commitMessage": "Deploy Storybook"
  }
}
