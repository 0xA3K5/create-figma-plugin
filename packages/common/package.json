{
  "name": "@create-figma-plugin/common",
  "version": "0.0.85-alpha.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "Internal utilities used across the `create-figma-plugin` monorepo",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/common"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "yarn run clean-build && tsc --build",
    "clean": "yarn run clean-build && yarn run clean-test",
    "clean-build": "rm -rf '*.log' lib tsconfig.tsbuildinfo",
    "clean-test": "rm -rf '*.log' .nyc_output coverage",
    "fix": "eslint --fix '{src,test}/**/*.ts' && prettier --loglevel error --write '*.json' 'test/fixtures/**/*.json'",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "yarn run build && yarn run clean-test && tap 'test/*.ts' --coverage-report html --coverage-report text --no-browser --no-check-coverage --reporter terse --timeout 120 --ts"
  },
  "dependencies": {
    "@sindresorhus/slugify": "1.1.2",
    "fs-extra": "^9.1.0",
    "kleur": "^4.1.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.41",
    "@types/tap": "^15.0.0",
    "eslint": "^7.25.0",
    "eslint-config-yuanqing": "^0.0.4",
    "prettier": "^2.2.1",
    "tap": "^15.0.5",
    "typescript": "^4.2.4"
  }
}
