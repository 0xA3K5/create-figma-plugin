{
  "name": "create-figma-plugin",
  "version": "0.0.85-alpha.3",
  "description": "Initialize a new Figma plugin using a template",
  "keywords": [
    "create-figma-plugin",
    "figma",
    "figma-plugin",
    "figma-plugins"
  ],
  "license": "MIT",
  "author": "Yuan Qing Lim",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/create-figma-plugin.git",
    "directory": "packages/create-figma-plugin"
  },
  "files": [
    "lib",
    "plugin-templates"
  ],
  "bin": {
    "create-figma-plugin": "lib/cli.js"
  },
  "scripts": {
    "build": "yarn run clean-build && tsc --build",
    "clean": "rimraf docs && yarn run clean-build && yarn run clean-test",
    "clean-build": "rimraf '*.log' lib tsconfig.tsbuildinfo",
    "clean-test": "rimraf '*.log' 'test/fixtures/*/figma-plugin' .nyc_output coverage",
    "fix": "eslint --fix '{src,test}/**/*.ts' && prettier --loglevel error --write '*.json'",
    "lint": "eslint '{src,test}/**/*.ts'",
    "test": "yarn run build && yarn run clean-test && tap 'test/*.ts' --coverage-report html --coverage-report text --no-browser --no-check-coverage --timeout 300 --reporter terse --timeout 120 --ts"
  },
  "dependencies": {
    "@create-figma-plugin/common": "^0.0.85-alpha.3",
    "degit": "^2.8.4",
    "fs-extra": "^9.1.0",
    "git-user-name": "^2.0.0",
    "globby": "^11.0.3",
    "inquirer": "^8.0.0",
    "is-utf8": "^0.2.1",
    "latest-version": "^5.1.0",
    "mustache": "^4.2.0",
    "sade": "^1.7.4",
    "titlecase": "^1.1.3"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.19.3",
    "@types/degit": "^2.8.2",
    "@types/fs-extra": "^9.0.10",
    "@types/git-user-name": "^2.0.0",
    "@types/inquirer": "^7.3.1",
    "@types/mustache": "^4.1.1",
    "@types/node": "^14.14.37",
    "@types/rimraf": "^3.0.0",
    "@types/sade": "^1.7.2",
    "@types/tap": "^14.10.3",
    "eslint": "^7.24.0",
    "eslint-config-yuanqing": "^0.0.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tap": "^15.0.2",
    "typescript": "^4.2.4"
  }
}
