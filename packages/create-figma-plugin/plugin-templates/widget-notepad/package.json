{
  "dependencies": {
    "@create-figma-plugin/ui": "^{{{versions.ui}}}",
    "@create-figma-plugin/utilities": "^{{{versions.utilities}}}",
    "preact": "^10"
  },
  "devDependencies": {
    "@create-figma-plugin/build": "^{{{versions.build}}}",
    "@create-figma-plugin/tsconfig": "^{{{versions.tsconfig}}}",
    "@figma/plugin-typings": "{{{figmaPluginTypingsVersion}}}",
    "@figma/widget-typings": "{{{figmaWidgetTypingsVersion}}}",
    "typescript": "^4"
  },
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figjam"
    ],
    "containsWidget": true,
    "id": "{{{name}}}",
{{#displayName}}
    "name": "{{{displayName}}}",
{{/displayName}}
    "main": "src/main.tsx",
    "ui": "src/ui.tsx"
  }
}
